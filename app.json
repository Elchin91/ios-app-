{
  "expo": {
    "name": "M10 Wallet",
    "slug": "m10-wallet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.m10.wallet.app",
      "buildNumber": "1",
      "infoPlist": {
        "UIBackgroundModes": [],
        "NSCameraUsageDescription": "This app uses the camera to scan QR codes."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
